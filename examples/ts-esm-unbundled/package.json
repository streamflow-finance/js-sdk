{
  "name": "ts-esm-unbundled",
  "version": "1.0.0",
  "description": "Example of using Streamflow SDK with TypeScript, ESM, and no bundler",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start-node18": "node dist/start-node18.js",
    "start-node20": "node dist/start-node20.js",
    "verify": "npm run build && node dist/runner.js",
    "validate-types": "tsc --noEmit"
  },
  "dependencies": {
    "@streamflow/stream": "workspace:*",
    "@streamflow/common": "workspace:*",
    "@streamflow/staking": "workspace:*",
    "@streamflow/distributor": "workspace:*",
    "@streamflow/launchpad": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
