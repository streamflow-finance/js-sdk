{
  "name": "ts-cjs-unbundled",
  "version": "1.0.0",
  "description": "Example of using Streamflow SDK with TypeScript, CommonJS, and no bundler",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "verify": "npm run build && node dist/index.js",
    "validate-types": "tsc --noEmit"
  },
  "dependencies": {
    "@streamflow/stream": "workspace:*",
    "@streamflow/common": "workspace:*",
    "@streamflow/staking": "workspace:*",
    "@streamflow/distributor": "workspace:*",
    "@streamflow/launchpad": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
